@model FunWithGit.ViewModels.QuoteViewModel
@{
}
@section Scripts {
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <h1>Found quote</h1>
        <form method="get">
            @if (ViewBag.Message != null)
            {
                <div>@ViewBag.Message</div>
            }
            <div class="form-group">
                <h1><label asp-for="FirstName">First Name</label></h1>
                <p>
                    <label asp-for="FirstName">@Model.FirstName</label>
                </p>
            </div>

            <div class="form-group">
                <label asp-for="LastName">Last Name</label>
                <p>
                    <label asp-for="LastName">@Model.LastName</label>
                </p>
            </div>

            <div class="form-group">
                <label asp-for="Email">Email</label>
                <p>
                    <label asp-for="Email">@Model.Email</label>
                </p>
            </div>
        </form>
    </div>
</div>